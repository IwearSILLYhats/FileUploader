<div class="contentCard" data-id="<%= data.id %>" data-type="<%= data.type %>">
  <div>
    <img src="/images/folder.svg" alt="" />
    <h5 class="cardName"><%= data.name %></h5>
    <p>Shared Icon</p>
    <button class="optionsBtn">
      <img src="/images/3dots.svg" alt="" />
    </button>
  </div>
  <ul class="contentOptions">
    <li>
      <a href="/<%= data.type %>/<%= data.id %>">
        <button class="openBtn">Open</button>
      </a>
    </li>
    <li>
      <button class="renameBtn">Rename</button>
    </li>
    <li><button class="moveBtn">Move</button></li>
    <li><button class="shareBtn">Share</button></li>
    <li><button class="deleteBtn">Delete</button></li>
  </ul>
</div>
